{
  "name": "@naikamu/mobile",
  "version": "0.11.3",
  "apiVersion": "1.3.x",
  "private": true,
  "engines": {
    "node": "18.17.1"
  },
  "engineStrict": true,
  "scripts": {
    "android:staging": "react-native run-android --mode stagingdebug",
    "android:staging-release": "react-native run-android --mode stagingrelease",
    "android:dev": "react-native run-android --mode developmentdebug",
    "android:dev-release": "react-native run-android --mode developmentrelease",
    "android:prod": "react-native run-android --mode productiondebug",
    "android:prod-release": "react-native run-android --mode productionrelease",
    "ios": "react-native run-ios --mode \"Debug\" --scheme \"NaikamuDev\" --simulator \"iPhone 15\"",
    "ios:release": "react-native run-ios --mode \"Release\" --scheme \"NaikamuDev\" --simulator \"iPhone 15\"",
    "ios:stg:release": "react-native run-ios --mode \"Release\" --scheme \"NaikamuStg\"",
    "ios:prod:release": "react-native run-ios --mode \"Release\" --scheme \"Naikamu\"",
    "pod": "npx pod-install ios",
    "start": "react-native start --reset-cache",
    "start:clear": "cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "app:release": "cd android && ./gradlew bundleRelease",
    "apk:beta:release": "cd android && fastlane beta && cd .. && npm run apk:beta:rename && open android/app/build/outputs/apk/production/release",
    "apk:release": "cd android && ./gradlew assembleRelease && cd .. && npm run apk:beta:rename && open android/app/build/outputs/apk/production/release",
    "apk:stg:release": "cd android && ./gradlew assembleStaging && cd .. && npm run apk:stg:rename && open android/app/build/outputs/apk/staging/release",
    "apk:rename": "mv $DIR/$APP $DIR/naikamu-$RENV$(jq -r '.version' package.json).apk",
    "apk:beta:rename": "DIR='android/app/build/outputs/apk/production/release' APP='app-production-release.apk' RENV= npm run apk:rename",
    "apk:stg:rename": "DIR='android/app/build/outputs/apk/staging/release' APP='app-staging-release.apk' RENV=stg- npm run apk:rename"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.19.8",
    "@react-native-community/blur": "4.3.2",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/analytics": "18.6.2",
    "@react-native-firebase/app": "18.6.2",
    "@react-native-firebase/auth": "18.6.2",
    "@react-native-firebase/crashlytics": "18.6.2",
    "@react-native-google-signin/google-signin": "10.1.1",
    "@react-native-picker/picker": "^2.5.1",
    "@react-navigation/bottom-tabs": "6.5.11",
    "@react-navigation/drawer": "6.6.6",
    "@react-navigation/material-top-tabs": "6.6.5",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@sentry/react-native": "5.14.1",
    "@tanstack/react-query": "4.35.7",
    "axios": "1.6.2",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "i18next": "23.7.6",
    "react": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-i18next": "13.5.0",
    "react-native": "0.72.7",
    "react-native-config": "1.5.1",
    "react-native-fast-image": "8.6.3",
    "react-native-fs": "2.20.0",
    "react-native-gesture-handler": "2.14.0",
    "react-native-keychain": "8.1.2",
    "react-native-linear-gradient": "2.8.3",
    "react-native-localize": "3.0.4",
    "react-native-media-console": "2.2.0",
    "react-native-pager-view": "6.2.2",
    "react-native-paper": "5.11.2",
    "react-native-picker-select": "^9.0.0",
    "react-native-qrcode-svg": "6.2.0",
    "react-native-reanimated": "3.5.4",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "3.29.0",
    "react-native-shadow-2": "7.0.8",
    "react-native-svg": "14.0.0",
    "react-native-system-navigation-bar": "2.6.3",
    "react-native-tab-view": "3.5.2",
    "react-native-vector-icons": "10.0.2",
    "react-native-video": "5.2.1",
    "react-native-webview": "13.6.3",
    "semver": "7.5.4",
