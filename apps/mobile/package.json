{
  "name": "@aniwatch/mobile",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "please-use-yarn",
    "yarn": "3"
  },
  "engineStrict": true,
  "scripts": {
    "android:staging": "react-native run-android --variant=stagingdebug --appId=com.aniwatch.stg",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=developmentdebug --appId=com.aniwatch.dev",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug --appId=com.aniwatch",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios": "react-native run-ios",
    "pod": "npx pod-install ios",
    "start": "react-native start",
    "start:clear": "cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "app:release": "cd android && ./gradlew bundleRelease",
    "apk:beta:release": "cd android && fastlane beta && open app/build/outputs/apk/production/release",
    "apk:release": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/production/release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-firebase/analytics": "^16.4.2",
    "@react-native-firebase/app": "^16.4.2",
    "@react-native-firebase/auth": "^16.3.0",
    "@react-native-firebase/crashlytics": "^16.4.2",
    "@react-native-google-signin/google-signin": "^8.2.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@reduxjs/toolkit": "^1.8.6",
    "@tanstack/react-query": "^4.3.9",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "i18next": "^22.0.2",
    "react": "18.2.0",
    "react-hook-form": "^7.38.0",
    "react-i18next": "^12.0.0",
    "react-native": "0.71.1",
    "react-native-config": "^1.4.11",
    "react-native-dotenv": "^3.3.1",
    "react-native-fast-image": "^8.6.1",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-keychain": "^8.1.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localize": "^2.2.4",
    "react-native-media-console": "^2.0.6",
    "react-native-paper": "^5.3.1",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-shadow-2": "^7.0.5",
    "react-native-svg": "^13.2.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-native-webview": "^11.23.1",
    "react-redux": "^8.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.69.5",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-native-video": "^5.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
