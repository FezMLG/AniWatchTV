{
  "name": "@aniwatch/mobile",
  "version": "0.7.1",
  "apiVersion": "1.0.x",
  "private": true,
  "engines": {
    "node": "18"
  },
  "engineStrict": true,
  "scripts": {
    "android:staging": "react-native run-android --variant=stagingdebug --appId=com.aniwatch.stg",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=developmentdebug --appId=com.aniwatch.dev",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug --appId=com.aniwatch",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios": "react-native run-ios --simulator=\"iPhone 14\" ",
    "ios:release": "npm run ios -- --mode=\"Release\"",
    "pod": "npx pod-install ios",
    "start": "react-native start --reset-cache",
    "start:clear": "cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "app:release": "cd android && ./gradlew bundleRelease",
    "apk:beta:release": "cd android && fastlane beta && open app/build/outputs/apk/production/release",
    "apk:release": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/production/release",
    "apk:stg:release": "cd android && ./gradlew assembleStaging && open app/build/outputs/apk/staging/release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.19.3",
    "@react-native-community/blur": "4.3.2",
    "@react-native-community/netinfo": "9.4.1",
    "@react-native-firebase/analytics": "18.4.0",
    "@react-native-firebase/app": "18.4.0",
    "@react-native-firebase/auth": "18.4.0",
    "@react-native-firebase/crashlytics": "18.4.0",
    "@react-native-google-signin/google-signin": "10.0.1",
    "@react-navigation/bottom-tabs": "6.5.8",
    "@react-navigation/drawer": "6.6.3",
    "@react-navigation/material-top-tabs": "6.6.3",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/native-stack": "6.9.13",
    "@tanstack/react-query": "4.35.3",
    "axios": "1.5.0",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "i18next": "23.5.1",
    "react": "18.2.0",
    "react-hook-form": "7.46.2",
    "react-i18next": "13.2.2",
    "react-native": "0.72.4",
    "react-native-config": "1.5.1",
    "react-native-fast-image": "8.6.3",
    "react-native-fs": "2.20.0",
    "react-native-gesture-handler": "2.13.1",
    "react-native-keychain": "8.1.2",
    "react-native-linear-gradient": "2.8.3",
    "react-native-localize": "3.0.2",
    "react-native-media-console": "2.0.7",
    "react-native-pager-view": "6.2.1",
    "react-native-paper": "5.10.6",
    "react-native-qrcode-svg": "6.2.0",
    "react-native-reanimated": "3.5.4",
    "react-native-safe-area-context": "4.7.2",
    "react-native-screens": "3.25.0",
    "react-native-shadow-2": "7.0.8",
    "react-native-svg": "13.14.0",
    "react-native-system-navigation-bar": "2.6.1",
    "react-native-tab-view": "3.5.2",
    "react-native-vector-icons": "10.0.0",
    "react-native-video": "5.2.1",
    "react-native-webview": "13.6.0",
    "semver": "7.5.4",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@babel/preset-env": "7.22.20",
    "@babel/runtime": "7.22.15",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "0.72.11",
    "@tsconfig/react-native": "3.0.2",
    "@types/jest": "29.5.5",
    "@types/react": "18.2.22",
    "@types/react-native": "0.72.2",
    "@types/react-native-vector-icons": "6.4.14",
    "@types/react-native-video": "5.0.15",
    "@types/react-test-renderer": "18.0.2",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.49.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-unicorn": "48.0.1",
    "jest": "29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.2.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
