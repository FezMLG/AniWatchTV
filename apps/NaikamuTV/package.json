{
  "name": "@naikamu/tv",
  "version": "0.3.0",
  "apiVersion": "1.5.x",
  "codeVersion": "13",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "android:dev": "react-native run-android --mode debug",
    "ios": "expo run:ios",
    "tvos": "expo run:ios --scheme NaikamuTV-tvOS --device \"Apple TV\"",
    "lint": "eslint .",
    "start": "expo start",
    "start:native": "react-native start --reset-cache",
    "start:clear": "cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "test": "jest",
    "apk:release": "cd android && ./gradlew assemble && cd .. && npm run apk:beta:rename && open android/app/build/outputs/apk/release",
    "apk:stg:release": "cd android && ./gradlew assemble && cd .. && npm run apk:stg:rename && open android/app/build/outputs/apk/release",
    "apk:rename": "mv $DIR/$APP $DIR/naikamutv-beta-$RENV$VER.apk",
    "apk:beta:rename": "DIR='android/app/build/outputs/apk/release' APP='app-release.apk' RENV=v VER=$(jq -r '.version' package.json) npm run apk:rename",
    "apk:stg:rename": "DIR='android/app/build/outputs/apk/release' APP='app-release.apk' RENV=stg-r VER=$(jq -r '.codeVersion' package.json) npm run apk:rename"
  },
  "dependencies": {
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-firebase/app": "19.0.1",
    "@react-native-firebase/auth": "19.0.1",
    "@react-native-google-signin/google-signin": "11.0.0",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@sentry/react-native": "5.20.0",
    "@tanstack/react-query": "5.28.6",
    "axios": "1.6.8",
    "date-fns": "3.6.0",
    "expo": "50.0.14",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "i18next": "23.10.1",
    "react": "18.2.0",
    "react-i18next": "14.1.0",
    "react-native": "npm:react-native-tvos@0.73.6-0",
    "react-native-config": "1.5.1",
    "react-native-fast-image": "8.6.3",
    "react-native-localize": "3.0.6",
    "react-native-media-console": "2.2.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-progress": "5.0.1",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.29.0",
    "react-native-svg": "15.1.0",
    "react-native-vector-icons": "10.0.3",
    "react-native-video": "6.0.0-beta.6",
    "styled-components": "6.1.8",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/preset-env": "7.24.3",
    "@babel/runtime": "7.24.1",
    "@react-native/babel-preset": "0.75.0-main",
    "@react-native/eslint-config": "0.74.75",
    "@react-native/metro-config": "0.74.75",
    "@react-native/typescript-config": "0.74.75",
    "@tsconfig/react-native": "3.0.3",
    "@types/react": "18.2.68",
    "@types/react-native-vector-icons": "6.4.18",
    "@types/react-native-video": "5.0.20",
    "@types/react-test-renderer": "18.0.7",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-styled-components": "2.1.4",
    "eslint": "8.57.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "react-native-svg-transformer": "1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.4.3"
  },
  "engines": {
    "node": "20.11.1"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
